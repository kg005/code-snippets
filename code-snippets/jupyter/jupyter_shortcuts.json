    // List of Keyboard Shortcuts
    "shortcuts": [
        {
            "command": "application:toggle-mode",
            "keys": [
                "Accel Shift D"
            ],
            "selector": "body",
            "disabled": true
        },
        {
            "command": "kernelmenu:interrupt",
            "keys": [
                "I",
                "I"
            ],
            "selector": "[data-jp-kernel-user]:focus",
            "disabled": true
        },
        {
            "command": "kernelmenu:restart",
            "keys": [
                "0",
                "0"
            ],
            "selector": "[data-jp-kernel-user]:focus",
            "disabled": true
        },
        {
            "command": "notebook:hide-cell-code",
            "keys": [
                "I"
            ],
            "selector": ".jp-Notebook:focus"
        },
        {
            "command": "notebook:show-cell-code",
            "keys": [
                "Shift I"
            ],
            "selector": ".jp-Notebook:focus"
        },
        {
            "command": "notebook:hide-all-cell-code",
            "keys": [
                "I",
                "I"
            ],
            "selector": ".jp-Notebook:focus"
        },
        {
            "command": "notebook:show-all-cell-code",
            "keys": [
                "Shift I",
                "Shift I"
            ],
            "selector": ".jp-Notebook:focus"
        },
        {
            "command": "notebook:hide-cell-outputs",
            "keys": [
                "O"
            ],
            "selector": ".jp-Notebook:focus"
        },
        {
            "command": "notebook:show-cell-outputs",
            "keys": [
                "Shift O"
            ],
            "selector": ".jp-Notebook:focus"
        },
        {
            "command": "notebook:hide-all-cell-outputs",
            "keys": [
                "O",
                "O"
            ],
            "selector": ".jp-Notebook:focus"
        },
        {
            "command": "notebook:show-all-cell-outputs",
            "keys": [
                "Shift O",
                "Shift O"
            ],
            "selector": ".jp-Notebook:focus"
        },
        {
            "command": "notebook:clear-cell-output",
            "keys": [
                "Ctrl Alt O"
            ],
            "selector": ".jp-Notebook:focus"
        },
        {
            "command": "notebook:clear-all-cell-outputs",
            "keys": [
                "Ctrl Shift Alt O"
            ],
            "selector": ".jp-Notebook:focus"
        },
        {
            "command": "notebook:render-all-markdown",
            "keys": [
                "Alt M"
            ],
            "selector": ".jp-Notebook:focus"
        },
        {
            "command": "notebook:enable-output-scrolling",
            "keys": [
                "S"
            ],
            "selector": ".jp-Notebook:focus"
        },
        {
            "command": "notebook:disable-output-scrolling",
            "keys": [
                "Shift S"
            ],
            "selector": ".jp-Notebook:focus"
        },
        {
            "command": "notebook:paste-cell-above",
            "keys": [
                "Shift V"
            ],
            "selector": ".jp-Notebook:focus"
        },
        {
            "command": "notebook:move-cell-up",
            "keys": [
                "Ctrl ArrowUp"
            ],
            "selector": ".jp-Notebook:focus"
        },
        {
            "command": "notebook:move-cell-down",
            "keys": [
                "Ctrl ArrowDown"
            ],
            "selector": ".jp-Notebook:focus"
        },
        {
            "command": "viewmenu:line-numbering",
            "keys": [
                "Alt N"
            ],
            "selector": ".jp-Notebook.jp-mod-commandMode"
        },
        {
            "command": "jupyterlab_code_formatter:black",
            "keys": [
                "Ctrl Alt L"
            ],
            "selector": ".jp-Notebook.jp-mod-editMode"
        },
        {
            "command": "jupyterlab_code_formatter:black",
            "keys": [
                "Ctrl Alt L"
            ],
            "selector": ".jp-Notebook:focus"
        },
        {
            "command": "application:toggle-presentation-mode",
            "keys": [
                "Alt Q"
            ],
            "selector": "body"
        },
        {
            "command": "filebrowser:download",
            "keys": [
                "Alt Shift D"
            ],
            "selector": ".jp-FileBrowser"
        },
        {
            "command": "filebrowser:delete",
            "keys": [
                "Shift Delete"
            ],
            "selector": ".jp-FileBrowser"
        },
        {
            "command": "filebrowser:cut",
            "keys": [
                "Ctrl X"
            ],
            "selector": ".jp-FileBrowser"
        },
        {
            "command": "filebrowser:copy",
            "keys": [
                "Ctrl Shift C"
            ],
            "selector": ".jp-FileBrowser"
        },
        {
            "command": "filebrowser:paste",
            "keys": [
                "Ctrl Shift V"
            ],
            "selector": ".jp-FileBrowser"
        },
        {
            "command": "filebrowser:create-new-directory",
            "keys": [
                "Ctrl Shift N"
            ],
            "selector": ".jp-FileBrowser"
        },
        {
            "command": "filebrowser:rename",
            "keys": [
                "F2"
            ],
            "selector": ".jp-FileBrowser"
        },
// ------------------------------------------------------------------------------------------
        {
            // activates next tab in the order as tabs are shown in the tab bar
            "command": "application:activate-next-tab",
            "keys": [
                
                "Ctrl Shift ."
            ],
            "selector": "body"
        },
        {   
            // activates previous tab in the order as tabs are shown in the tab bar
            "command": "application:activate-previous-tab",
            "keys": [
                "Ctrl Shift ,"
            ],
            "selector": "body"
        },
        // ^
        // |
        // !! These two pairs have their meaning opposit to what their name would suggest !! 
        // |
        // v
        {
            // activates next tab in last used order
            "command": "application:activate-next-tab-bar",
            "keys": [
                "Alt `"
            ],
            "selector": "body"
        },
        {
            // activates previous tab in last used order
            "command": "application:activate-previous-tab-bar",
            "keys": [
                "Alt `" // same as above as we can switch to the previously used tab only (history size = 1tab)
            ],
            "selector": "body"
        },
// ------------------------------------------------------------------------------------------
    ]
}